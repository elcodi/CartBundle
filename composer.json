{
    "name": "elcodi/cart-bundle",
    "description": "Elcodi Cart component",
    "keywords": ["elcodi", "cart", "ecommerce"],
    "homepage": "https://github.com/elcodi/CartBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/CartBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "symfony/config": "~2.4",
        "symfony/event-dispatcher": "~2.4",
        "symfony/dependency-injection": "~2.4",

        "elcodi/core-bundle": "~0",
        "elcodi/user-bundle": "~0",
        "elcodi/product-bundle": "~0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "Elcodi/Bundles/Core/CartBundle",
    "autoload": {
        "psr-0": { "Elcodi\\Bundles\\Core\\CartBundle": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}